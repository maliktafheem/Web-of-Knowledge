@article{ ISI:000459709500092,
Author = {Yang, Yan and Dang, Shuping and He, Yejun and Guizani, Mohsen},
Title = {{Markov Decision-Based Pilot Optimization for 5G V2X Vehicular
   Communications}},
Journal = {{IEEE INTERNET OF THINGS JOURNAL}},

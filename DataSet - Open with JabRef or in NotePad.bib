@article{ ISI:000442317300009,
Author = {He, Daojing and Chan, Sammy and Guizani, Mohsen},
Title = {{Security in the Internet of Things Supported by Mobile Edge Computing}},
Journal = {{IEEE COMMUNICATIONS MAGAZINE}},

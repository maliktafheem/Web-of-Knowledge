@article{ ISI:000441428700021,
Author = {Zhu, Liehuang and Zhang, Chuan and Xu, Chang and Du, Xiaojiang and Xu,
   Rixin and Sharif, Kashif and Guizani, Mohsen},
Title = {{PRIF: A Privacy-Preserving Interest-Based Forwarding Scheme for Social
   Internet of Vehicles}},
Journal = {{IEEE INTERNET OF THINGS JOURNAL}},
